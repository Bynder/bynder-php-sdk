{
  "name": "bynder/bynder-php-sdk",
  "description": "Bynder PHP Library",
  "version": "2.1.3",
  "keywords": [
    "bynder",
    "sdk",
    "api"
  ],
  "homepage": "https://www.bynder.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Bynder",
      "homepage": "https://github.com/Bynder/bynder-php-sdk"
    }
  ],
  "require": {
    "php": ">= 5.6",
    "guzzlehttp/guzzle": "~6.0",
    "league/oauth2-client": "^2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "mikey179/vfsstream": "~1",
    "phpdocumentor/phpdocumentor": "2.*",
    "php-coveralls/php-coveralls": "^2.1",
    "phpunit/phpcov": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Bynder\\": "src/Bynder"
    }
  },
  "scripts": {
    "test":[
      "@putenv XDEBUG_MODE=coverage",
      "vendor/bin/phpunit tests -c phpunit.xml.dist --whitelist src"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Bynder\\Tests\\": "tests"
    }
  }
}
